<body class="cbp-spmenu-push">
	<div class="main-content">
		<div class="cbp-spmenu cbp-spmenu-vertical cbp-spmenu-left" id="cbp-spmenu-s1">
			<!--left-fixed -navigation-->
			<%- include('sidebar'); -%>
		</div>
		<!--left-fixed -navigation-->

		<!-- header-starts -->
		<%- include('header'); -%>
			<!-- //header-ends -->
			<!-- main content start-->
			<div id="page-wrapper">
				<div class="main-page">
					<div class="forms">

						<div class="form-grids row widget-shadow" data-example-id="basic-forms">
							<div class="form-title">
								<h4>Edit Category</h4>
							</div>
							<div class="form-body">
								<form method="POST" action="/editusr/<%= data[0].user_id%>">
									<table>
										<tr>
											<td>Name</td>
											<td><input type="text" name = "txt1" value="<%= data[0].name %>" ></td>
										</tr>
										<tr>
											<td>Gender</td>
											<td><input type="text" name = "txt2" value="<%= data[0].gender %>" ></td>
										</tr>
										<tr>
											<td>Email</td>
											<td><input type="email" name = "txt3" value="<%= data[0].email %>" ></td>
										</tr>
										<tr>
											<td>Phone</td>
											<td><input type="text" name = "txt4" value="<%= data[0].phone %>" ></td>
										</tr>
										<tr>
											<td>Address</td>
											<td><input type="text" name = "txt5" value="<%= data[0].address %>" ></td>
										</tr>
										<tr>
											<td>Pasword</td>
											<td><input type="password" name = "txt6" value="<%= data[0].password %>" ></td>
										</tr>
										<tr>
											<td><input type="submit"></td>
											
										</tr>
									</table>
								</form>
								<!-- <a href="/table/displaycategory">display</a> -->
							</div>
						</div>

					</div>
				</div>
			</div>
			<!--footer-->
			<%- include('footer'); -%>
				<!--//footer-->
	</div>

	<!-- side nav js -->
	<script src='/js/SidebarNav.min.js' type='text/javascript'></script>
	<script>
		$('.sidebar-menu').SidebarNav()
	</script>
	<!-- //side nav js -->

	<!-- Classie -->
	<!-- for toggle left push menu script -->
	<script src="/js/classie.js"></script>
	<script>
		var menuLeft = document.getElementById('cbp-spmenu-s1'),
			showLeftPush = document.getElementById('showLeftPush'),
			body = document.body;

		showLeftPush.onclick = function () {
			classie.toggle(this, 'active');
			classie.toggle(body, 'cbp-spmenu-push-toright');
			classie.toggle(menuLeft, 'cbp-spmenu-open');
			disableOther('showLeftPush');
		};

		function disableOther(button) {
			if (button !== 'showLeftPush') {
				classie.toggle(showLeftPush, 'disabled');
			}
		}
	</script>
	<!-- //Classie -->
	<!-- //for toggle left push menu script -->

	<!--scrolling js-->
	<script src="/js/jquery.nicescroll.js"></script>
	<script src="/js/scripts.js"></script>
	<!--//scrolling js-->

	<!-- Bootstrap Core JavaScript -->
	<script src="/js/bootstrap.js"> </script>

</body>

